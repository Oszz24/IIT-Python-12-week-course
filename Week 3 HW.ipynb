    "name = input(\"Enter your name: \")\n",
    "marks = float(input(\"Enter your marks: \"))\n",
    "\n",
    "if marks > 80:\n",
    "    grade = \"A\"\n",
    "elif marks >= 60:\n",
    "    grade = \"B\"\n",
    "elif marks >= 40:\n",
    "    grade = \"C\"\n",
    "else:\n",
    "    grade = \"Fail\"\n",
    "\n",
    "print(\"Name:\", name)\n",
    "print(\"Marks:\", marks)\n",
    "print(\"Grade:\", grade)"
   ]
